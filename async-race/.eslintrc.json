{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "airbnb-typescript/base"],
  "parserOptions": {
    "project": "**/tsconfig.json"
  },
  "rules": {
    "no-use-before-define":["error", { "functions": false}],
    "@typescript-eslint/no-use-before-define": ["error", { "functions": false}]
  }
}
